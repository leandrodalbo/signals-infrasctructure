[
    {
      "name": "${ecs_service_name}",
      "image": "${image_url}",
      "essential": true,
      "environment": [{
        "name": "spring_profiles_active",
        "value": "${spring_profile_active}"
      }],
      "portMappings": [{
        "containerPort": ${container_port}
      }],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${ecs_service_name}-logs",
          "awslogs-region": "${region}",
          "awslogs-stream-prefix": "${ecs_service_name}-logs"
        }
      }
    }
  ]