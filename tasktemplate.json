[
  {
    "name": "${task_definition_name}",
    "image": "${image_url}",
    "essential": true,
    "environment": [
      {
        "name": "spring_profile_active",
        "value": "${spring_profile_active}"
      }
    ],
    "portMappings": [{"containerPort":${docker_container_port}}],
    "logConfiguration": {
        "logDriver":"awslogs",
        "options": {
            "awslogs-group": "${ecs_service_name}-log-group",
            "awslogs-region": "${region}"
        }
    }
  }
]
